  "sideEffects": false,
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "Firefox ESR",
    "not dead",
    "not IE 11"
  ],
  "volta": {
    "node": "20.11.0",
    "npm": "10.2.4"
  },
  "packageManager": "npm@10.2.4",
  "overrides": {},
  "resolutions": {},
  "betterScripts": {
    "dev": {
      "command": "nodemon server.js",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    }
  },
  "nodemonConfig": {
    "watch": [
      "server.js",
      "src/",
      "public/"
    ],
    "ext": "js,json,html,css",
    "ignore": [
      "node_modules/",
      "tests/",
      "docs/"
    ],
    "delay": 1000,
    "verbose": true
  },
  "prettier": {
    "semi": true,
    "singleQuote": false,
    "tabWidth": 2,
    "useTabs": false,
    "printWidth": 100,
    "trailingComma": "none",
    "arrowParens": "avoid"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "env": {
      "node": true,
      "es2022": true
    },
    "parserOptions": {
      "ecmaVersion": 2022,
      "sourceType": "commonjs"
    },
    "rules": {
      "no-console": "off",
      "no-unused-vars": "warn"
    }
  },
  "jest": {
    "testEnvironment": "node",
    "coverageDirectory": "./coverage",
    "collectCoverageFrom": [
      "src/**/*.js",
      "server.js"
    ],
    "testMatch": [
      "**/tests/**/*.test.js"
    ]
  },
  "nyc": {
    "reporter": [
      "html",
      "text",
      "lcov"
    ],
    "exclude": [
      "tests/",
      "node_modules/",
      "coverage/"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm test"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run lint",
      "npm run format"
    ]
  },
  "security": {
    "audit": true,
    "advisories": {
      "moderate": "warn",
      "high": "error",
      "critical": "error"
    }
  },
  "performance": {
    "budget": {
      "bundle": "500kb",
      "initial": "200kb"
    }
  },
